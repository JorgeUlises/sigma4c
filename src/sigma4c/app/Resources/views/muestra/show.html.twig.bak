{% extends 'base.html.twig' %}

{% block body %}
    <h1>Muestra</h1>

    <table>
        <tbody>
            <tr>
                <th>Responsable</th>
                <td>{{ muestra.responsable }}</td>
            </tr>
            <tr>
                <th>Elementoambiental</th>
                <td>{{ muestra.elementoAmbiental }}</td>
            </tr>
            <tr>
                <th>Fotos</th>
                <td>{{ muestra.fotos }}</td>
            </tr>
            <tr>
                <th>Fechatoma</th>
                <td>{% if muestra.fechaToma %}{{ muestra.fechaToma|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Fecharecepcion</th>
                <td>{% if muestra.fechaRecepcion %}{{ muestra.fechaRecepcion|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Fechaanalisis</th>
                <td>{% if muestra.fechaAnalisis %}{{ muestra.fechaAnalisis|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Tipomuestreo</th>
                <td>{{ muestra.tipoMuestreo }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ muestra.id }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('muestra_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('muestra_edit', { 'id': muestra.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
